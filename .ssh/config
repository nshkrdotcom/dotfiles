# Default GitHub account (nshkrdotcom) using main key
Host n
  Hostname github.com
  User git
  IdentityFile ~/.ssh/id_rsa
  # Optional: Add this if you have issues with known_hosts
  # StrictHostKeyChecking no
  # UserKnownHostsFile /dev/null

# Second GitHub account (vibecd) using a different key
Host v
  Hostname github.com
  User git
  IdentityFile ~/.ssh/id_rsa_vibecd
  # Optional: Add this if you have issues with known_hosts
  # StrictHostKeyChecking no
  # UserKnownHostsFile /dev/null


Host g
  Hostname github.com
  User git
  IdentityFile ~/.ssh/id_rsa_gtcode-dot-com


Host f
  Hostname github.com
  User git
  IdentityFile ~/.ssh/id_rsa_nshkr-factory-dot-ai


